heap.begin_push(1)
heap.push((1, 0), 1)
heap.push((3, 1), 2)
heap.push((2, 2), 3)
heap.push((4, 3), 3)
# heap.push((5, 4), 4)
heap.end_push()
heap.print()
top = heap.pop()
heap.print()
check(top, (1, 0))
top = heap.pop()
heap.print()
check(top, (2, 2))
